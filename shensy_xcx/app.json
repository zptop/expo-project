{
  "expo": {
    "name": "申丝司机",
    "slug": "shensy_xcx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "允许$(PRODUCT_NAME)获取您的位置以提供导航服务"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*",
      "components/**/*",
      "page/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shensy.driver",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "需要获取您的位置信息以提供导航服务",
        "NSLocationAlwaysUsageDescription": "需要获取您的位置信息以提供导航服务",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "需要获取您的位置信息以提供导航服务",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "AMapApiKey": "218cfd67a9c19db9a4588d5b47d5e1df"
      },
      "config": {
        "amap": {
          "apiKey": "218cfd67a9c19db9a4588d5b47d5e1df"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shensy.driver",
      "versionCode": 1,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "amap": {
          "apiKey": "218cfd67a9c19db9a4588d5b47d5e1df"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "c5d6a2a9-b8a7-4ee2-a41b-06a5029b2cee"
      }
    },
    "owner": "zp_top"
  }
}