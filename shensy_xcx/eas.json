{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "local",
    "requireCommit": false,
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_NETWORK_TIMEOUT": "300000"
      }
    },
    "release": {
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_NETWORK_TIMEOUT": "300000"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
